{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "downlevelIteration": true,
    "lib": ["dom", "es2015"],
    "module": "commonjs",
    "noImplicitReturns": true,
    "strict": true,
    "target": "es5",
    "outDir": "dist",
    "rootDir": ".",
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "skipLibCheck": true
  },
  "include": ["./src/types"],
  "files": []
}
